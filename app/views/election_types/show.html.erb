<% provide(:title, @election_type.name) %>
<h1>Election Type: <%= @election_type.name %></h1>

<div class="field">
  <label class="label">Name</label>
  <%= @election_type.name %>
</div>

<div class="buttons">
  <%= link_to 'Edit', edit_election_type_path(@election_type), class: "button is-link" %>
  <%= link_to 'Delete', @election_type, method: :delete, data: { confirm: 'Are you sure?' }, class: "button is-danger" %>
  <%= link_to 'Back', election_types_path, class: "button" %>
</div>