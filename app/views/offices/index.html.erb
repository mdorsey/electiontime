<% provide(:title, 'Offices') %>
<h1>Offices</h1>

<div class="columns">
  <div class="column is-narrow">
    <%= render 'admin/admin_menu' %>
  </div>
  <div class="column">
    <%= render 'admin/admin_list', new_object_text: "+ New Office", new_object_path: new_office_path, use_turbolinks: "false",
      object_count: @offices.count, objects_path: offices_path, placeholder_text: "Search by name...", objects_list: @offices %>
  </div>
</div>