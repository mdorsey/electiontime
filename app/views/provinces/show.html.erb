<% provide(:title, @province.name) %>
<h1>Province: <%= @province.name %></h1>

<p>
  <strong>Name: </strong><%= @province.name %>
</p>

<%= link_to 'Edit', edit_province_path(@province), class: "btn btn-primary" %>
<%= link_to 'Delete', @province, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %>
<%= link_to 'Back', provinces_path, class: "btn btn-default" %>