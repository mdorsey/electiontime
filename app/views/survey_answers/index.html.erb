<% provide(:title, 'Survey Answers') %>
<h1>Survey Answers</h1>

<div class="columns">
  <div class="column is-narrow">
    <%= render 'admin/admin_menu' %>
  </div>
  <div class="column">
    <%= render 'admin/admin_list', new_object_text: "+ New Survey Answer", new_object_path: new_survey_answer_path, use_turbolinks: "true",
      object_count: @survey_answers.count, objects_path: survey_answers_path, placeholder_text: "Search by answer...", objects_list: @survey_answers %>
  </div>
</div>